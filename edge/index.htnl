<html>
	<head>
		<link rel="shortcut  icon" href="bjsdfz.ico" type="image/x-icon" />
		<meta charset="utf-8">
		<title>az-edge</title>
        <style type="text/css" >
            .sr{
width: 1800px;
text-align:center;
height: 25px;
position: fixed;
top: 1%;
left:80px;
            }
    .sx{
        position: fixed;
        top: 1%;
        left:20px;
    }
    .lb{
    width: 270px;
	height: 40px;
	}
    </style>
        <script type="text/javascript">
function sub(){
                var content = document.getElementById("txt");
                var element=document.getElementById("lb");
                element.innerHTML="<iframe marginwidth='0' marginheight='0' width='100%' height='95%' src='"+content.value+"' frameborder='no'></iframe>"
                }
</script>
<body background="viewfile.jpg">
    <div  class="lb">
    <div class="sr">
    <input id="txt" value="https://bing.com/" style="width:100%;height:25px"/>
</div> 
    <div class="sx">
    <input type="button" value="刷新" onclick="sub()">
    </div>
   </div>
    <div id="lb">
        <iframe marginwidth="0" marginheight='0' width="100%" height="95%" src="https://bing.com/" frameborder="no"></iframe>
    </div>
</body>
